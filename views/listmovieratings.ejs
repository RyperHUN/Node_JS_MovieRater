    <table class="table table-striped">
        <thead>
          <tr>
            <th>Rank</th>
            <th>Title</th>
            <th>Rating</th>
            <th> ID </th>
            <th></th>
          </tr>
        </thead>
        <tbody>
            <% var i = 1; %>
            <% for (key in concat) { var item = concat[key]; %>
            <tr>
                <td><%= i %>.</td>
                 <td><%= item.name %></td> <!--TODO ADD LINK FOR DETAILEDMOVIE -->
                <td><%= item.rating %>/10</td>
                <td><%= item.movieId %></td>
                <td><form action="/rate/mode/<%= item.movieId %>"
                      <button type="button" class="btn btn-primary btn-xs">Rate</button>
                    </form>
                </td>
              </tr>
              <%i = i + 1;%>
            <% } %>
        </tbody>
      </table>
  </div>